        <div id="DZ_W_Notification1" class="widget-media dlab-scroll p-3">
            <ul class="timeline">
                
                @foreach (var notification in Model)
                {
                    <li>
                        <div class="timeline-panel">
                            <div class="media me-2 media-@notification.NotificationType.Color">
