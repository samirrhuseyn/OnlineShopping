@model EntityLayer.Concrete.Store
@{
    ViewData["Title"] = "StoreDetails";
    Layout = "~/Views/Shared/_publicLayout.cshtml";

}

<div style="margin-left:50px;" class="col-lg-11">
    <div class="shop-filters mt-30">
        <img style="padding:0px,20px 0px, 20px; margin-left:50px; height:130px; width:160px;" src="@Model.LogoImage" width="500" />

       
        <div style="margin-right:850px;">
            <h4>@Model.Name</h4>
            <a role="button" class="btn btn-primary mb-2" data-bs-toggle="modal" data-bs-target=".bd-example-modal-lg" style="-webkit-text-fill-color:white;" href="#"><i style="color:white" class="ion ion-md-chatbubbles"></i> Mesaj göndər</a>
        </div>
        <div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">@Model.Name mağazasına ismarıc göndər!</h5>
                    </div>
                    <div class="modal-body">
                        <div class="col-lg-12">
                            <div class="commentbox">
                                <form id="contact-form" action="/Store/SendMessage/" method="POST" class="ho-form contact-form">
                                    <div class="ho-form-inner">
                                        <div class="single-input">
                                            <label for="new-review-name">Başlıq</label>
                                            <input type="text" name="MessageTitle" id="new-review-name" required>
                                        </div>
                                        <div class="single-input single-input-half">
                                            <input type="text" name="StoreID" value="@Model.StoreID" id="new-review-email" hidden>
                                        </div>
                                        
                                        <div class="single-input">
                                            <label for="new-review-textbox">İsmarıcınız</label>
                                            <textarea id="new-review-textbox" required name="MessageBody" cols="30" rows="5"></textarea>
                                        </div>
                                        <div class="single-input">
                                            <button class="ho-button ho-button-dark" type="submit"><span>Göndər</span></button>
                                        </div>
                                    </div>
                                </form>
                                <p class="form-message"></p>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-danger light" data-bs-dismiss="modal">Ləğv et</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
